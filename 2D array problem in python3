if __name__ == '__main__':
    a = []

    for _ in range(6):
        a.append(list(map(int, input().rstrip().split())))
        
    sum=[]
    for i in range(0,4):
        for j in range(0,4):
            s=a[i][j]+a[i][j+1]+a[i][j+2]+a[i+1][j+1]+a[i+2][j]+a[i+2][j+1]+a[i+2][j+2]
            sum.append(s)
            
            
    print(max(sum))
        
